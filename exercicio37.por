programa {
  funcao inicio() {
    // Crie um algoritmo que leia dois números e calcule a média.
    real numero1, numero2, media
    escreva("escreva um numero: ")
    leia(numero1)
    escreva("escreva outro numero: ")
    leia(numero2)
    media=numero1+numero2
    escreva("a média dos seus numeros são: ", media/2)

  }
}
